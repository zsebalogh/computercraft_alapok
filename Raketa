local util = dofile("util.lua")

function szarny1()
	util.place("planks")
	turtle.back()
	util.place("planks")
	turtle.back()
	util.place("stairs")
	turtle.up()
	turtle.forward()
	turtle.forward()
	util.place("planks")
	turtle.back()
	util.place("stairs")
	turtle.up()
	turtle.forward()
	util.place("stairs")
end

function szarny2()
	util.place("planks")
	turtle.back()
	util.place("stairs")
	turtle.up()
	turtle.forward()
	util.place("planks")
	turtle.up()
	util.place("stairs")
end
